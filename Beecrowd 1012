a,b,c  = list(map(float,input().split()))
rectangled_triangle=.5*a*c
radius_circle = 3.14159 * c*c
trapezium = .5*(a+b)*c
square = b*b
rectangle = a*b
print("TRIANGULO: %.3f"%rectangled_triangle)
print("CIRCULO: %.3f"%radius_circle)
print("TRAPEZIO: %.3f"%trapezium)
print("QUADRADO: %.3f"%square)
print("RETANGULO: %.3f"%rectangle)
